# xAI API Configuration
# Get your API key from https://platform.x.ai
VITE_XAI_API_KEY=your_xai_api_key_here

# Optional: xAI API Base URL (default: https://api.x.ai/v1)
VITE_XAI_API_BASE=https://api.x.ai/v1

# Optional: Model configuration
VITE_XAI_MODEL=grok-code-fast-1

# Optional: AI Assistant Settings
VITE_AI_MAX_TOKENS=2000
VITE_AI_TEMPERATURE=0.3
VITE_AI_TOP_P=0.9

# Development Settings
VITE_APP_TITLE="AGK Web IDE"
VITE_APP_VERSION="1.0.0"

# Optional: Enable debug logging
VITE_DEBUG=false